# Supabase Configuration
# Get these from your Supabase project settings: https://supabase.com/dashboard/project/_/settings/api
REACT_APP_SUPABASE_URL=your-supabase-project-url
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key

# OpenAI Configuration
# Get your API key from: https://platform.openai.com/api-keys
REACT_APP_OPENAI_API_KEY=your-openai-api-key

# Sentry Configuration (Optional - for error tracking)
# Get DSN from: https://sentry.io/settings/projects/
REACT_APP_SENTRY_DSN=your-sentry-dsn
REACT_APP_SENTRY_DEBUG=false

# SendGrid Configuration (for email notifications)
# Get API key from: https://app.sendgrid.com/settings/api_keys
SENDGRID_API_KEY=your-sendgrid-api-key

# Twilio Configuration (for SMS notifications - optional)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=your-twilio-phone-number

# Application Configuration
NODE_ENV=development
REACT_APP_ENV=development

# API Base URLs (if using external APIs)
REACT_APP_API_BASE_URL=http://localhost:3000
